{
	"name": "discord-sponsorblock-player",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"main": "dist/app",
	"scripts": {
		"start": "node .",
		"dev": "node -r dotenv/config .",
		"debug": "npm run clean && (echo Building... & tsc --sourcemap) && node -r dotenv/config .",
		"test": "npm run build && node -r dotenv/config .",
		"build": "npm run clean && (echo Building... & tsc)",
		"clean": "echo Cleaning... & rd /s /q \"./dist\" & exit",
		"docker:start": "docker compose up --attach sbplayer",
		"docker:test": "docker compose up --build --attach sbplayer",
		"docker:build": "docker build -t origeva/sbplayer .",
		"docker:clean": "docker compose down",
		"docker:push": "npm run docker:build && echo Pushing image... & docker push origeva/sbplayer:latest",
		"prepublishOnly": "exit 1"
	},
	"keywords": [],
	"author": "Ori Geva",
	"license": "ISC",
	"dependencies": {
		"@discordjs/voice": "^0.14.0",
		"axios": "^0.24.0",
		"discord.js": "^14.7.1",
		"express": "^4.17.1",
		"ffmpeg-static": "^4.4.0",
		"libsodium-wrappers": "^0.7.11",
		"lodash": "^4.17.21",
		"luxon": "^2.1.1",
		"passport": "^0.5.0",
		"socket.io": "^4.4.0",
		"sponsorblock-api": "^0.2.2",
		"spotify-web-api-node": "^5.0.2",
		"typescript": "^4.9.5",
		"winston": "^3.3.3",
		"ytdl-core": "^4.9.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/ffmpeg-static": "^3.0.0",
		"@types/lodash": "^4.14.176",
		"@types/luxon": "^2.0.7",
		"@types/node": "^18.15.0",
		"@types/passport": "^1.0.7",
		"@types/spotify-web-api-node": "^5.0.3",
		"dotenv": "^10.0.0"
	}
}
