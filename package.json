{
	"name": "discord-sponsorblock-player",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"main": "dist/app",
	"scripts": {
		"start": "node .",
		"dev": "node -r ts-node/register -r dotenv/config src",
		"debug": "npm run clean && (echo Building... & tsc --sourcemap)",
		"test": "npm run build && node -r dotenv/config .",
		"build": "npm run clean && (echo Building... & tsc)",
		"clean": "echo Cleaning... & rd /s /q \"./dist\" & exit",
		"docker:start": "docker run -p 80:8080 -it --rm --name sbplayer origeva/sbplayer",
		"docker:test": "npm run dockerize && npm run docker:start",
		"dockerize": "npm run build && npm run docker:clean & (echo Building image... & docker build -t origeva/sbplayer .)",
		"docker:clean": "echo Cleaning image... & docker image rm origeva/sbplayer & exit",
		"docker:push": "npm run dockerize && echo Pushing image... & docker push origeva/sbplayer:latest",
		"prepublishOnly": "exit 1"
	},
	"keywords": [],
	"author": "Ori Geva",
	"license": "ISC",
	"dependencies": {
		"@discordjs/builders": "^0.8.2",
		"@discordjs/voice": "^0.7.5",
		"axios": "^0.24.0",
		"discord.js": "^13.3.1",
		"express": "^4.17.1",
		"ffmpeg-static": "^4.4.0",
		"libsodium-wrappers": "^0.7.9",
		"lodash": "^4.17.21",
		"luxon": "^2.1.1",
		"passport": "^0.5.0",
		"socket.io": "^4.4.0",
		"sponsorblock-api": "^0.2.2",
		"spotify-web-api-node": "^5.0.2",
		"winston": "^3.3.3",
		"youtube-dl-exec": "^1.3.3",
		"ytdl-core": "^4.9.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/ffmpeg-static": "^3.0.0",
		"@types/lodash": "^4.14.176",
		"@types/luxon": "^2.0.7",
		"@types/passport": "^1.0.7",
		"@types/spotify-web-api-node": "^5.0.3",
		"dotenv": "^10.0.0",
		"nodemon": "^2.0.12",
		"ts-node": "^10.2.0"
	}
}
